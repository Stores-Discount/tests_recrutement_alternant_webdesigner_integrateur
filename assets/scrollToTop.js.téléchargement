$(document).ready(function(){$(window).scroll(function(){$(this).scrollTop()>200?$(".scrollup").fadeIn():$(".scrollup").fadeOut()}),$(".scrollup").click(function(e){e.preventDefault(),console.log("scrollup"),$("html, body").animate({scrollTop:0},700)}),$(window).scroll(function(){$(this).scrollTop()>0?$("#header-wrap").css("position","fixed !important"):$("#header-wrap").css("position","initial")})});