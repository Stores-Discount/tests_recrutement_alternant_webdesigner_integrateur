function isItMobileTablet(){var result=!1,width=0;return width=window.outerWidth?window.outerWidth:document.body.offsetWidth,navigator.userAgent.match(/Android|BlackBerry|iPhone|iPad|iPod|Opera Mini|IEMobile/i)&&(width>767?result="tablet":width<768&&(result="mobile")),result}var getHeight=function(el){var wanted_height,el_style=window.getComputedStyle(el),el_display=el_style.display,el_position=el_style.position,el_visibility=el_style.visibility,el_max_height=el_style.maxHeight.replace("px","").replace("%","");return"none"!==el_display&&"0"!==el_max_height?el.offsetHeight:(el.style.position="absolute",el.style.visibility="hidden",el.style.display="block",wanted_height=el.offsetHeight,el.style.display=el_display,el.style.position=el_position,el.style.visibility=el_visibility,wanted_height)};function slideUp(element,duration){return new Promise(function(resolve,reject){element.style.height=element.offsetHeight+12+"px",element.style.transitionDuration=duration+"ms",element.offsetHeight,element.style.overflow="hidden",element.style.height=0,element.style.paddingTop=0,element.style.paddingBottom=0,element.style.marginTop=0,element.style.marginBottom=0,window.setTimeout(function(){element.style.display="none",element.style.removeProperty("height"),element.style.removeProperty("padding-top"),element.style.removeProperty("padding-bottom"),element.style.removeProperty("margin-top"),element.style.removeProperty("margin-bottom"),element.style.removeProperty("overflow"),element.style.removeProperty("transition-duration"),element.style.removeProperty("transition-property"),resolve(!1)},duration)})}function slideDown(element,duration){return new Promise(function(resolve,reject){element.style.removeProperty("display");var display=window.getComputedStyle(element).display;"none"===display&&(display="block"),element.style.display=display;var height=element.scrollHeight;element.style.overflow="hidden",element.style.height=0,element.style.paddingTop=0,element.style.paddingBottom=0,element.style.marginTop=0,element.style.marginBottom=0,element.offsetHeight,element.style.transitionDuration=duration+"ms",element.style.height=height+"px",element.style.removeProperty("padding-top"),element.style.removeProperty("padding-bottom"),element.style.removeProperty("margin-top"),element.style.removeProperty("margin-bottom"),window.setTimeout(function(){element.style.removeProperty("height"),element.style.removeProperty("overflow"),element.style.removeProperty("transition-duration"),element.style.removeProperty("transition-property")},duration)})}function slideToggle(element,duration){return"none"===window.getComputedStyle(element).display?slideDown(element,duration):slideUp(element,duration)}function scrollToElement(destination){ua=navigator.userAgent,ua.indexOf("MSIE")>-1||ua.indexOf("Trident")>-1||ua.indexOf("Edge")>-1?window.scrollTo(0,destination.offsetTop):window.scrollTo({top:destination.offsetTop,behavior:"smooth"})}function trimToPx(elm,length){var tmp=elm,trimmed=elm;if(tmp.scrollWidth>length)for(trimmed+="...";trimmed.scrollWidth>length;)trimmed=(tmp=tmp.substring(0,tmp.length-1))+"...";return trimmed}function menuTabClick(e){var mainElement,toActivate;if((window.outerWidth?window.outerWidth:document.body.offsetWidth)>767){if(toActivate=getTarget(mainElement=e.target.parentElement),mainElement.classList.contains("menuTabItem")&&!mainElement.classList.contains("active")){var activeCategory;if(activeCategory=document.querySelector(".menuTabItem.active")){var activeQuestion=document.querySelector(activeCategory.getAttribute("data-target"));activeQuestion&&(activeQuestion.classList.remove("active"),"response"==activeQuestion.children[1].classList&&(activeQuestion.children[1].style.display="none")),activeCategory.classList.remove("active"),getTarget(activeCategory).classList.remove("active")}mainElement.classList.add("active"),toActivate.classList.add("active")}}else toActivate=getTarget(mainElement=e.target.parentElement.classList.contains("menuTabItem")?e.target.parentElement:e.target),(mainElement.classList.contains("menuTabItem")||mainElement.parentElement.classList.contains("menuTabItem"))&&(mainElement.classList.contains("active")?(mainElement.classList.remove("active"),toActivate.classList.remove("active")):((activeCategory=document.querySelector("div.menuTabItem.active"))&&(activeCategory.classList.remove("active"),getTarget(activeCategory).classList.remove("active")),mainElement.classList.add("active"),toActivate.classList.add("active")))}function getTarget(elm){var id=elm.getAttribute("data-target");return document.querySelector(id)}function mobileTAB(){var target,categoriesDiv=Array.from(document.querySelectorAll("div.menuTabItem")),categories=Array.from(document.querySelectorAll("li.menuTabItem")),activeCategory=document.querySelector("li.menuTabItem.active"),width=0;if((width=window.outerWidth?window.outerWidth:document.body.offsetWidth)<768&&0===categoriesDiv.length){var i=0;categories.forEach(function(elm){target=getTarget(elm);var divTitle=document.createElement("div");divTitle.className="menuTabItem",elm.classList.contains("active")?(elm.classList.remove("active"),divTitle.classList.add("active"),i+=1):activeCategory||0!==i||(divTitle.classList.add("active"),i+=1),divTitle.innerHTML=elm.innerHTML,divTitle.setAttribute("data-target",elm.getAttribute("data-target")),cgvZone.insertBefore(divTitle,target)})}else width>767&&(categoriesDiv.forEach(function(elm,index){elm.classList.contains("active")&&(activeCategory=categories[index]).classList.add("active"),elm.parentElement.removeChild(elm)}),activeCategory||(activeCategory=categories[0]),getTarget(activeCategory).classList.add("active"))}function hideShowElement(toHide,toShow){toHide&&(slideUp(toHide,500),toHide.parentElement.classList.remove("active")),toShow&&(slideDown(toShow,500),toShow.parentElement.classList.add("active"))}function smoothScroll(e){var elm=!1;(e.target.classList.contains("anchor")?elm=e.target:e.target.parentElement.classList.contains("anchor")&&(elm=e.target.parentElement),elm)&&scrollToElement(document.querySelector(elm.getAttribute("data-target")))}function insertElement(newElement,refElement,position){"before"===position?refElement.parentNode.insertBefore(newElement,refElement):refElement.parentNode.insertBefore(newElement,refElement.nextSibling)}function getMenuList(list){var menuOptions=[];return Array.from(list.children).forEach(function(element){menuOptions.push({optionText:element.textContent,optionTarget:element.getAttribute("data-target")})}),menuOptions}function makeMenuList(list,position){var activeIndex=0,menuOptions=getMenuList(list),mobileMenu=document.createElement("div");mobileMenu.className="cc-menu";var selectedMenu=document.createElement("div");selectedMenu.className="cc-menu__text";var arrowMenu=document.createElement("div");arrowMenu.className="cc-menu__arrow";var arrowIcon=document.createElement("div");arrowIcon.className="arrowSelect",arrowIcon.innerHTML='<i class="icon-chevron_up_bold"></i>',arrowMenu.appendChild(arrowIcon);var ulMenu=document.createElement("ul");ulMenu.className="cc-menu__list",menuOptions.forEach(function(item,index){var optionMenu=document.createElement("li");optionMenu.className="cc-menu__item",optionMenu.innerText=item.optionText,optionMenu.setAttribute("data-target",item.optionTarget),document.querySelector(item.optionTarget).classList.contains("active")&&(activeIndex=index),ulMenu.appendChild(optionMenu)});var optionMenu=ulMenu.children[activeIndex];optionMenu.classList.add("current"),selectedMenu.innerText=optionMenu.innerText,document.querySelector(optionMenu.getAttribute("data-target")).classList.add("active"),mobileMenu.appendChild(selectedMenu),mobileMenu.appendChild(arrowMenu),mobileMenu.appendChild(ulMenu),insertElement(mobileMenu,list,position)}function makeMenuTabForMobile(currentList,parent){var width=0;if(width=window.outerWidth?window.outerWidth:document.body.offsetWidth,("mobile"===isItMobileTablet()||width<768)&&!document.querySelector(".menuTab")){var newMenu=document.createElement("ul");newMenu.className="menuTab",Array.from(currentList.children).forEach(function(element){if(element.classList.contains("categoryAnchor")){var li=document.createElement("li");li.className="menuTabItem",li.setAttribute("data-target","#"+element.getAttribute("data-target")),Array.from(element.children).forEach(function(subElm){subElm.classList.contains("infoTitle")?li.textContent=subElm.textContent:Array.from(subElm.children).length>0&&Array.from(subElm.children).forEach(function(lastElm){lastElm.classList.contains("infoTitle")&&(li.textContent=lastElm.textContent)})}),newMenu.appendChild(li)}}),insertElement(newMenu,currentList,"after")}else"mobile"!==isItMobileTablet()&&document.querySelector(".menuTab")&&(parent.removeChild(document.querySelector(".menuTab")),parent.removeChild(document.querySelector(".cc-menu")))}function showSubMenus(e){if(e.target.classList.contains("cc-menu__text")||e.target.classList.contains("cc-menu__arrow")||e.target.parentElement.classList.contains("arrowSelect")||e.target.parentElement.classList.contains("cc-menu__arrow")){var menuContainer=document.querySelector(".withCCMenu"),menu=document.querySelector(".cc-menu"),distance=document.querySelector(".responsive-footer").offsetTop-menu.offsetTop;if(menu.classList.contains("cc-menu--opened"))menu.classList.remove("cc-menu--opened"),menuContainer.style.paddingBottom="0";else{menu.classList.add("cc-menu--opened");var minHeight=parseInt(48*document.querySelectorAll(".cc-menu__item").length)+150;menuContainer.style.paddingBottom=distance<minHeight?minHeight-distance+"px":"0"}}}function selectMenu(e){if(e.target.classList.contains("cc-menu__item")){var link=e.target;if(document.querySelector(".withCCMenu").style.paddingBottom="0",!link.classList.contains("current")){var selectedMenuZone=document.querySelector(".cc-menu__text"),currentMenu=document.querySelector("li.current");currentMenu.classList.remove("current");var currentTarget=currentMenu.getAttribute("data-target");document.querySelector(currentTarget).classList.remove("active");var target=link.getAttribute("data-target"),text=link.textContent;selectedMenuZone.innerHTML=text,link.classList.add("current"),document.querySelector(target).classList.add("active"),document.querySelector(".cc-menu").classList.remove("cc-menu--opened")}}}function treatMenuScreenChanges(){var mobileMenu;(window.outerWidth?window.outerWidth:document.body.offsetWidth)<768?document.querySelector(".cc-menu")||document.querySelector(".menuTab")&&makeMenuList(document.querySelector(".menuTab"),"after"):(mobileMenu=document.querySelector(".cc-menu"))&&mobileMenu.parentElement.removeChild(mobileMenu)}document.addEventListener("DOMContentLoaded",function(){document.querySelector(".menuTab")&&(document.querySelector(".menuTab").addEventListener("click",menuTabClick),treatMenuScreenChanges(),window.addEventListener("resize",treatMenuScreenChanges),window.outerWidth||window.addEventListener("orientationchange",treatMenuScreenChanges),document.querySelector("body").addEventListener("click",smoothScroll));document.addEventListener("click",showSubMenus),document.addEventListener("click",selectMenu)});