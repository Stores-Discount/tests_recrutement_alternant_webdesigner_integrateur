var hashReload=!0;function preventHashReload(){hashReload=!1}!function(){function obfuscationManagement(e,data){var target=e.target;for(data.target=target,data.newTab=!1,data.link=null;target&&"BODY"!==target.nodeName;){if("A"===target.nodeName)return!0;if(void 0!==target.dataset&&"obflink"in target.dataset){var uri=decodeURIComponent(escape(window.atob(target.dataset.obflink)));return data.link=uri,"obftarget"in target.dataset||e.ctrlKey||void 0!==e.button&&1===e.button?(e.preventDefault(),data.newTab=!0,!1):void 0}target=target.parentNode}}function triggerRedirection(e,data){if(e.target===data.target&&data.link){if(data.newTab)return e.preventDefault(),window.open(data.link,"_blank"),!1;var oldPath=window.location.pathname,oldHash=window.location.hash;window.location.href=data.link,hashReload&&oldPath==window.location.pathname&&oldHash!=window.location.hash&&document.location.reload()}return data.target=null,data.newTab=!1,data.link=null,!0}var y,data;data={target:null,newTab:!1,link:null},document.addEventListener("mousedown",function(e){return 2==e.button&&"INPUT"!==e.target.nodeName?(e.preventDefault(),!1):obfuscationManagement(e,data)}),document.addEventListener("mouseup",function(e){return 2!=e.button&&data.link||"INPUT"===e.target.nodeName?triggerRedirection(e,data):(e.preventDefault(),!1)}),document.addEventListener("touchstart",function(e){return y=e.pageY,obfuscationManagement(e,data)}),document.addEventListener("touchend",function(e){if(y===e.pageY)return triggerRedirection(e,data)}),document.addEventListener("scroll",function(e){data.target=null})}();